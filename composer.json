{
  "name": "docforge/framework",
  "version": "0.0.15",
  "scripts": {
    "build:docs": "php build_docs.php vendor/docforge/handbook",
    "serve": "php -S localhost:8080 -t vendor/docforge/handbook/public"
  },
  "autoload": {
    "psr-4": {
      "DocForge\\Framework\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DocForge\\Framework\\Tests\\": "tests/"
    }
  },
  "require": {
    "erusev/parsedown": "^1.7",
    "webmozart/glob": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "docforge/handbook": "dev-master"
  }
}
